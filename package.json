{
  "name": "opendbs",
  "version": "0.1.0",
  "description": "OpenDBS - High-performance database with Node.js API and Rust storage engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "database",
    "nosql",
    "sql",
    "json",
    "rust",
    "performance",
    "vector-search",
    "fuzzy-search"
  ],
  "author": "OpenDBS Team",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^9.4.0",
    "@fastify/rate-limit": "^10.0.1",
    "@fastify/static": "^9.0.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "basic-ftp": "^5.1.0",
    "bcrypt": "^6.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "fastify": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "msgpackr": "^1.11.8",
    "mysql2": "^3.16.1",
    "node-cron": "^4.2.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "sql-query-identifier": "^2.9.0",
    "zod": "^3.22.4",
    "@napi-rs/cli": "^2.18.4"
  },
  "devDependencies": {},
  "napi": {
    "name": "opendbs-engine",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu"
      ]
    }
  }
